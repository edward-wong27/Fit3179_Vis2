{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/edward-wong27/Fit3179_Vis2/main/data/ACCIDENT.csv"},
  "selection": {
"highlight": {
"type": "multi",
"fields": ["Vehicle Type Desc"],
"bind": "legend"
}
},
  "mark": "bar",
  "encoding": {
    "x": {
      "timeUnit": "month",
      "field": "Date",
      "type": "ordinal",
      "title": "Month of the year"
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative",
      "title": "Total Crashes"
    },
      "color": {
      "field": "Vehicle Type Desc",
      "type": "nominal",
      "scale": {
        "domain": ["Bicycle", "Bus/Coach", "Motor Cycle", "Other Vehicle", "Taxi","Train","Tram", "Truck", "Unknown", "Car"],
        "range": ["#e7ba52", "#c7c7c7", "#aec7e8", "#1f77b4", "#9467bd","#9767bd"]
      },
      "title": "Vehicle type"

    }
  }
}
