{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "title": "Victorian Crashes",
    "projection": {"type": "mercator"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/edward-wong27/Git_Repository/main/vic_localities.topojson",
          "format": {"type": "topojson", "feature": "vic_localities"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/edward-wong27/Git_Repository/main/data/NODE.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "Long", "type": "quantitative"},
          "latitude": {"field": "Lat", "type": "quantitative"}
        }
      }
    ],
    "config": {}
  }